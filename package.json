{
  "name": "googletopten",
  "version": "1.0.0",
  "description": "Generate images for the Google Top Ten search results",
  "main": "app.js",
  "bin": {
    "googletopten": "app.js"
  },
  "scripts": {
    "build": "npm run clean && tsc",
    "clean": "shx rm -rf build",
    "start": "npm run build && node app.js images --debug ",
    "prepare": "npm run build",
    "publish": "echo Published.",
    "install": "tsc && echo Installed.",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kfaubel/googleTopTen.git"
  },
  "dependencies": {
    "@types/express": "^4.17.6",
    "axios": "^0.21.0",
    "pureimage": "^0.2.5",
    "jpeg-js": "^0.4.2",
    "express": "^4.17.3",
    "ts-node-dev": "^1.0.0-pre.44",
    "typescript": "^4.0.0",
    "winston": "^3.2.1",
    "xml-js": "^1.6.11",
    "xml2js": "^0.4.23"
  },
  "devDependencies": {
    "@types/node": "^12.12.22",
    "prettier": "^1.19.1",
    "tslint": "^5.20.1",
    "tslint-config-prettier": "^1.18.0",
    "shx": "^0.3.2"
  },
  "author": "Ken Faubel",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kfaubel/googleTopTen/issues"
  },
  "homepage": "https://github.com/kfaubel/googleTopTen#readme"
}
